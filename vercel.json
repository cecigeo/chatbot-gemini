{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
   "routes": [
    {
      "src": "^/chat(?:$|/.*)$",
      "methods": ["OPTIONS","GET","POST"],
      "dest": "api/api.py"
    }
  ],
  "excludeFiles": [
    "**/*.pyc",
    "**/__pycache__/**",
    ".env/",
    ".git/**",
    ".envbot**",
    ".vercelignore**",
    "intentst.json**",
    "training/**",
    "models/**",
    "db.py**",
    "main.py",
    "README.md",
    ".vercel",
    "package.json"
  ]
}
