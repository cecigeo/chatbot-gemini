{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python@3.11"
    }
  ],
  "routes": [
     {
      "src": "/chat/welcome",
      "methods": ["OPTIONS","GET"],
      "dest": "api/api.py"
    },
    {
      "src": "/chat",
      "methods": ["OPTIONS","POST"],
      "dest": "api/api.py"
    }
  ]
}
