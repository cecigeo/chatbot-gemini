{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
     {
      "src": "/chat/welcome",
      "methods": ["OPTIONS","GET"],
      "dest": "api.py"
    },
    {
      "src": "/chat",
      "methods": ["OPTIONS","POST"],
      "dest": "api.py"
    }
  ]
}
